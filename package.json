{
  "name": "markreader",
  "version": "1.0.0",
  "description": "Markdown reader and docs viewer",
  "author": "Sandro Lain",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/umd/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "npx rollup -c",
    "watch": "npx rollup -cw",
    "test": "jest --runInBand",
    "test:coverage": "jest --runInBand --coverage",
    "test:coverage:open": "jest --runInBand --coverage && open ./coverage/lcov-report/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sandrolain/markreader.git"
  },
  "keywords": [
    "markdown",
    "wiki"
  ],
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-html": "^0.2.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.13",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "git-rev-sync": "^3.0.1",
    "i": "^0.3.6",
    "jest": "^26.6.3",
    "npm": "^6.14.10",
    "rollup": "^2.34.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/highlightjs": "^10.1.0",
    "@types/showdown": "^1.9.3",
    "axios": "^0.21.0",
    "front-matter": "^4.0.2",
    "github-markdown-css": "^4.0.0",
    "highlight.js": "^10.4.1",
    "lit-html": "^1.3.0",
    "showdown": "^1.9.1",
    "sweet-scroll": "^4.0.0",
    "yaml": "^1.10.0"
  }
}
